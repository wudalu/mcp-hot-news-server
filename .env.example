# MCP Hot News Server API配置
# 复制此文件为 .env 并填入真实的API密钥

# Google Trends (通过SerpAPI获取)
SERPAPI_KEY=your_serpapi_key

# NewsAPI - 全球新闻
NEWSAPI_KEY=your_newsapi_key

# Reddit API
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=MCP_Hot_News_Server/1.0

# Twitter API (可选，支持多个第三方服务)
# TwitterAPI.io
TWITTER_API_IO_TOKEN=your_twitter_api_io_token

# Zyla API Hub Twitter
ZYLA_API_KEY=your_zyla_api_key

# RapidAPI Twitter
RAPIDAPI_KEY=your_rapidapi_key

# 日志级别
LOG_LEVEL=INFO 